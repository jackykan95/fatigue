<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="example.android.jacky.fatigue">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="com.google.android.gms.permission.ACTIVITY_RECOGNITION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".FirebaseLoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ActivitiesDaily"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity
            android:name=".ActivitiesWeekly"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity
            android:name=".EnergyLevelDaily"
            android:theme="@style/Theme.AppCompat.NoActionBar"/>
        <activity
            android:name=".MentalFatigueDaily"
            android:theme="@style/Theme.AppCompat.NoActionBar"/>
        <activity android:name=".MainActivity" />
        <activity
            android:name=".EnergyLevelWeekly"
            android:theme="@style/Theme.AppCompat.NoActionBar"/>
        <activity
            android:name=".MentalFatigueWeekly"
            android:theme="@style/Theme.AppCompat.NoActionBar"/>
    </application>

</manifest>